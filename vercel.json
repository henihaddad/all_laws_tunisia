{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "functions": {
    "src/pages/api/**/*.js": {
      "runtime": "@vercel/node"
    }
  },
  "regions": ["fra1"],
  "public": true
}
